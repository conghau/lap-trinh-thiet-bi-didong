            <TextBlock Height="30" HorizontalAlignment="Left" Margin="458,255,0,0" Name="tblCauhoiB4" Text="" TextAlignment="Center" VerticalAlignment="Top" Width="231" Foreground="#FEFFFFFF" MouseLeftButtonDown="tblCauhoiB4_MouseLeftButtonDown" MouseLeave="tblCauhoiB4_MouseLeave" />
            <TextBlock Height="30" HorizontalAlignment="Left" Margin="458,208,0,0" Name="tblCauhoiB5" Text="" TextAlignment="Center" VerticalAlignment="Top" Width="231" Foreground="#FEFFFFFF" MouseLeftButtonDown="tblCauhoiB5_MouseLeftButtonDown" MouseLeave="tblCauhoiB5_MouseLeave" />
        </Grid>

        <Grid Name="grdDacbiet" Visibility="Collapsed">
            <Image Height="480" HorizontalAlignment="Left" Name="imgDialogDacbiet" Stretch="Fill" VerticalAlignment="Top" Width="800" Source="/Images/dialog.png" />
            <TextBlock Height="138" HorizontalAlignment="Left" Margin="246,145,0,0" Name="tblCauHoiDacbiet" Text="Bạn có muốn trả lời câu hỏi đặc biêt?" VerticalAlignment="Top" Width="357" TextWrapping="Wrap" Style="{StaticResource PhoneTextTitle1Style}" FontFamily="iFunSoftFont.TTF#iFunSoftFont" FontSize="40" Foreground="#FEDEFF00" TextAlignment="Center" />
            <Image Height="49" HorizontalAlignment="Left" Margin="246,289,0,0" Name="imgCoDacbiet" Stretch="Fill" VerticalAlignment="Top" Width="161" Source="/Images/co1.png" MouseLeftButtonDown="imgCoDacbiet_MouseLeftButtonDown" />
            <Image Height="49" HorizontalAlignment="Right" Margin="0,289,197,0" Name="imgKhongDacbiet" Source="/Images/khong1.png" Stretch="Fill" VerticalAlignment="Top" Width="161" MouseLeftButtonDown="imgKhongDacbiet_MouseLeftButtonDown" />
        </Grid>

        <Grid Name="grdQuit" Visibility="Collapsed">
            <Image Height="480" HorizontalAlignment="Left" Name="imgDialogQuit" Stretch="Fill" VerticalAlignment="Top" Width="800" Source="/Images/dialog.png" />
            <TextBlock Height="138" HorizontalAlignment="Left" Margin="246,145,0,0" Name="tblCauHoiQuit" Text="Bạn có muốn dừng cuộc chơi?" VerticalAlignment="Top" Width="357" TextWrapping="Wrap" Style="{StaticResource PhoneTextTitle1Style}" FontFamily="iFunSoftFont.TTF#iFunSoftFont" FontSize="40" Foreground="#FEDEFF00" TextAlignment="Center" />
            <Image Height="49" HorizontalAlignment="Left" Margin="246,289,0,0" Name="imgCoQuit" Stretch="Fill" VerticalAlignment="Top" Width="161" Source="/Images/co1.png" MouseLeftButtonDown="imgCoQuit_MouseLeftButtonDown" MouseLeave="imgCoQuit_MouseLeave" />
            <Image Height="49" HorizontalAlignment="Left" Margin="442,289,0,0" Name="imgKhongQuit" Source="/Images/khong1.png" Stretch="Fill" VerticalAlignment="Top" Width="161" MouseLeftButtonDown="imgKhongQuit_MouseLeftButtonDown" MouseLeave="imgKhongQuit_MouseLeave" />
        </Grid>
    </Grid>
</phone:PhoneApplicationPage>